{
  "name": "deepsirius-ui",
  "private": true,
  "scripts": {
    "clean": "turbo clean",
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,md}\"",
    "type-check": "turbo type-check",
    "db:generate": "turbo -F @deepsirius-ui/app db:generate"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.5",
    "prettier": "^3.1.1",
    "turbo": "^2.0.11",
    "typescript": "^5.4.2"
  },
  "packageManager": "pnpm@9.6.0",
  "engines": {
    "node": ">=20"
  }
}